{
  "name": "@admenergy/next-common",
  "version": "1.0.0",
  "exports": {
    ".": {
      "types": "./dist/ts/index.d.ts",
      "import": "./dist/js/index.js",
      "require": "./dist/js/index.js"
    },
    "./client": {
      "types": "./dist/ts/client/index.d.ts",
      "import": "./dist/js/client/index.js",
      "require": "./dist/js/client/index.js"
    },
    "./common": {
      "types": "./dist/ts/common/index.d.ts",
      "import": "./dist/js/common/index.js",
      "require": "./dist/js/common/index.js"
    },
    "./server": {
      "types": "./dist/ts/server/index.d.ts",
      "import": "./dist/js/server/index.js",
      "require": "./dist/js/server/index.js"
    },
    "./ErrorTypes": {
      "types": "./dist/ts/ErrorTypes/index.d.ts",
      "import": "./dist/js/ErrorTypes/index.js",
      "require": "./dist/js/ErrorTypes/index.js"
    },
    "./components": {
      "types": "./dist/ts/components/index.d.ts",
      "import": "./dist/js/components/index.js",
      "require": "./dist/js/components/index.js"
    },
    "./hooks": {
      "types": "./dist/ts/hooks/index.d.ts",
      "import": "./dist/js/hooks/index.js",
      "require": "./dist/js/hooks/index.js"
    }
  },
  "webpackEntry": {
    ".": "./app/index.ts",
    "components": "./app/components/index.ts",
    "hooks": "./app/hooks/index.ts"
  },
  "private": false,
  "scripts": {
    "==[ Development ]==": "",
    "dev": "remix dev",
    "build": "yarn run clean && webpack --mode production && npx tsc",
    "build-git-add": "yarn run build && git add dist/",
    "build-test": "yarn run build && yarn run test",
    "test": "NODE_ENV=test vitest run --silent --config vitest.config.mts",
    "test:watch": "NODE_ENV=test vitest --config vitest.config.mts",
    "test:coverage": "NODE_ENV=test vitest run --coverage --config vitest.config.mts",
    "lint": "npx eslint app/ --config .eslintrc.js --ext .js,.jsx,.ts,.tsx --format json | node utility/eslint.js",
    "==[ Install/Clean ]==": "",
    "reinstall": "yarn run purge && yarn install && npx typesync",
    "clean": "rm -rf dist/ .next/ .cache/ build/ public/build/",
    "purge": "rm -rf dist/ .next/ node_modules/ package-lock.json yarn.lock .cache/ build/ public/build/"
  },
  "resolutions": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "**/react": "19.0.0",
    "**/react-dom": "19.0.0"
  },
